{
	"name": "cosmoz-grouped-list",
	"version": "1.0.0",
	"description": "[![Build Status](https://travis-ci.org/Neovici/cosmoz-grouped-list.svg?branch=master)](https://travis-ci.org/Neovici/cosmoz-grouped-list)",
	"main": "cosmoz-grouped-list.html",
	"directories": {
		"test": "test"
	},
	"devDependencies": {
		"eslint": "^5.12.0",
		"eslint-plugin-html": "^4.0.5",
		"eslint-plugin-mocha": "^5.2.1",
		"polymer-cli": "^1.9.5",
		"web-component-tester-istanbul": "Neovici/web-component-tester-istanbul"
	},
	"scripts": {
		"start": "polymer serve -o",
		"preinstall": "rm -f package-lock.json",
		"postinstall": "polymer install --variants && yarn sync-npm",
		"lint": "eslint --cache --ext .js,.html . && polymer lint cosmoz-*.html",
		"test": "polymer test",
		"analyze": "polymer analyze --input cosmoz-*.html  > analysis.json",
		"sync-npm": "npm shrinkwrap && mv npm-shrinkwrap.json package-lock.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neovici/cosmoz-grouped-list.git"
	},
	"keywords": [
		"polymer",
		"web-components"
	],
	"author": "",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/neovici/cosmoz-grouped-list/issues"
	},
	"homepage": "https://github.com/neovici/cosmoz-grouped-list#readme"
}
